         metrics_infiles=[gfdl_cm20_3050_file,gfdl_cm20_3050_file,gfdl_cm20_1020_file,gfdl_cm20_1020_file,gfdl_cm20_3050_file,gfdl_cm20_1020_file,gfdl_cm20_2120_file]
         clim_infile=gfdl_cm20_clim_file
         clim_start_read=180
         clim_fourd=0
         start_read=[180-50/2,180-50/2,180-20/2,180-20/2,180-50/2,180-20/2,180-120/2]
         plot_title='gfdl_cm2_0'
         n_days=60
         n_years=40
         multiplier=86400.
         variable='pr'
      END
      6: BEGIN
         metrics_infiles=[gfdl_cm21_3050_file,gfdl_cm21_3050_file,gfdl_cm21_1020_file,gfdl_cm21_1020_file,gfdl_cm21_3050_file,gfdl_cm21_1020_file,gfdl_cm21_2120_file]
         clim_infile=gfdl_cm21_clim_file
         clim_start_read=180
         clim_fourd=0
         start_read=[180-50/2,180-50/2,180-20/2,180-20/2,180-50/2,180-20/2,180-120/2]
         plot_title='gfdl_cm2_1'
         n_days=60

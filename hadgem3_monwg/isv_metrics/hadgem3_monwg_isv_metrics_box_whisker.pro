         metrics_infiles=[ncar_ccsm3_3050_file,ncar_ccsm3_3050_file,ncar_ccsm3_1020_file,ncar_ccsm3_1020_file,ncar_ccsm3_3050_file,ncar_ccsm3_1020_file,ncar_ccsm3_2120_file]

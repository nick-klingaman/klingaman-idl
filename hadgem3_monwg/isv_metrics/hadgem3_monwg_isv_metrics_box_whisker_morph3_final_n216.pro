;                  precip_clim_mean(m,n)=MEAN(clim_precip(m,n,*),/NaN)
               ENDIF ELSE $
                  precip_stddev(j,m,n)=!Values.F_NaN
;                  precip_clim_mean(m,n)=!Values.F_NaN
;               precip_stddev_scaled(j,m,n)=precip_stddev(j,m,n)/MEAN(clim_precip(m,n,*),/NaN)
            ENDFOR
